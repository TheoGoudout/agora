{
  "name": "agora",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "node-sass": "~3.8.0",
    "npm-watch": "~0.1.5",
    "uglify-js": "~2.7.0"
  },
  "watch": {
    "sass": "private/sass",
    "uglify-js": "private/js"
  },
  "scripts": {
    "sass": "node-sass private/sass --output public/css",
    "uglify-js": "uglifyjs private/js/*.js --output public/js/main.min.js --source-map public/js/main.min.js.map",
    "watch": "npm-watch"
  }
}
